{
	"name": "web",
	"private": true,
	"scripts": {
		"build": "next build",
		"dev": "next dev",
		"lint": "next lint",
		"start": "next start"
	},
	"eslintConfig": {
		"parserOptions": {
			"project": "./tsconfig.eslint.json"
		},
		"settings": {
			"react": {
				"version": "detect"
			}
		},
		"extends": [
			"neon/common",
			"neon/browser",
			"neon/typescript",
			"neon/react",
			"neon/next",
			"neon/prettier",
			"plugin:tailwindcss/recommended"
		],
		"rules": {
			"default-case": 0,
			"no-duplicate-imports": 2,
			"no-warning-comments": 0,
			"prefer-named-capture-group": 0,
			"@typescript-eslint/consistent-type-definitions": [
				2,
				"interface"
			],
			"@typescript-eslint/unbound-method": 0,
			"promise/prefer-await-to-callbacks": 0,
			"promise/prefer-await-to-then": 0,
			"tailwindcss/no-custom-classname": 0,
			"unicorn/consistent-function-scoping": 0
		},
		"ignorePatterns": [
			".next/*",
			".vercel/*",
			".yarn/*",
			"node_modules/*",
			"next-env.d.ts"
		],
		"root": true
	},
	"dependencies": {
		"next": "^13.4.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@types/node": "^18.16.12",
		"@types/react": "^18.2.6",
		"@types/react-dom": "^18.2.4",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.40.0",
		"eslint-config-neon": "^0.1.47",
		"eslint-plugin-tailwindcss": "^3.12.0",
		"postcss": "^8.4.23",
		"prettier": "^2.8.8",
		"tailwindcss": "^3.3.2",
		"typescript": "~5.0.4"
	}
}
